<html>
  <head>
    <title>Trave2020 Log</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  </head>
  <body>
      <div class="main-container">

        <div class="row container z-depth-4">
          <h3>Signup</h3>
    <form class="col s12" method="post" action="register/user">
      <div class="row">
        <div class="input-field col s6">
          <input placeholder="Placeholder" id="first_name" type="text" name="first_name" class="validate">
          <label for="first_name">First Name</label>
        </div>
        <div class="input-field col s6">
          <input id="last_name" type="text" name="last_name" class="validate">
          <label for="last_name">Last Name</label>
        </div>
        <div class="input-field col s12">
          <input id="user_email" type="email" name="email" class="validate">
          <label for="user_email">Email</label>
        </div>
        <div class="input-field col s12">
          <input id="username" type="text" name="username" name="username"class="validate">
          <label for="username">Username</label>
        </div>
        <div class="input-field col s12">
          <input id="user_password" type="password" name="user_password" class="validate">
          <label for="user_password">Password</label>
        </div>
      </div>
      <button class="btn waves-effect waves-light" type="submit" name="action">Submit
            <i class="material-icons right">send</i>
    </button>
    </form>
  </div>






        </div>
        <script> 
        </script>
     <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  </body>
</html>